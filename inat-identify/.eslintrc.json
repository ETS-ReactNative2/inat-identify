{
    "extends": [
      "airbnb",
      "plugin:flowtype/recommended"
    ],
    "plugins": [
      "flowtype",
      "react-native"
    ],
    "settings": {
      "flowtype": {
        "onlyFilesWithFlowAnnotation": false
      }
    },
    "env": {
      "react-native/react-native": true
    }
  }
